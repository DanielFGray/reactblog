{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "loose": true,
      "modules": false
    }],
    "stage-0",
    "react",
    "flow"
  ],
  "env": {
    "production": {
      "plugins": [
        ["lodash", { "id": [
          "ramda"
        ]}]
      ]
    },
    "development": {
      "plugins": [
        "flow-react-proptypes"
      ]
    }
  }
}
